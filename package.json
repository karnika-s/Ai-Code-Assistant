{
  "name": "chatbot",
  "displayName": "Chatbot",
  "description": "A chatbot extension for VS Code.",
  "version": "0.0.1",
  "publisher": "skychat",
  "engines": {
    "vscode": "^1.93.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:aiChatbot.sendMessage",
    "onCommand:aiChatbot.loadContext",
    "onCommand:aiChatbot.showWebview",
    "onView:aiChatbot.sidebar"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "aiChatbot.ask",
        "title": "Ask AI Chatbot"
      },
      {
        "command": "aiChatbot.loadContext",
        "title": "Load Context from File"
      },
      {
        "command": "aiChatbot.showWebview",
        "title": "Show Chatbot Webview"
      }
    ],
    "views": {
      "explorer": [
        {
          "id": "aiChatbot.sidebar",
          "name": "AI Chatbot"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.93.0",
    "@typescript-eslint/eslint-plugin": "^8.3.0",
    "@typescript-eslint/parser": "^8.3.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "dotenv-webpack": "^8.1.0",
    "eslint": "^9.9.1",
    "ts-loader": "^9.5.1",
    "typescript": "^5.5.4",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "dotenv": "^16.4.5",
    "groq-sdk": "^0.7.0"
  }
}
